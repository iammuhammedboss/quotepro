{
  "name": "quotepro-enhanced",
  "version": "2.0.0",
  "description": "Professional Quotation Management System with Advanced Export Features",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run build:css",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch",
    "setup": "npm install && npm run create-dirs",
    "create-dirs": "mkdir -p data/export-templates temp public/exports logs",
    "cleanup": "node scripts/cleanup.js",
    "backup": "node scripts/backup.js",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "quotation",
    "waterproofing",
    "construction",
    "business",
    "pdf",
    "excel",
    "export",
    "nodejs",
    "express"
  ],
  "author": {
    "name": "Muhammed Boss",
    "email": "muhammedboss@quotepro.com",
    "url": "https://quotepro.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iammuhammedboss/quotepro.git"
  },
  "bugs": {
    "url": "https://github.com/iammuhammedboss/quotepro/issues"
  },
  "homepage": "https://github.com/iammuhammedboss/quotepro#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "helmet": "^6.1.5",
    "mysql2": "^3.6.0",
    "puppeteer": "^21.6.1",
    "qrcode": "^1.5.3",
    "exceljs": "^4.4.0",
    "sharp": "^0.32.6",
    "archiver": "^5.3.1",
    "nodemailer": "^6.9.1",
    "multer": "^1.4.5-lts.1",
    "morgan": "^1.10.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "csv-parser": "^3.0.0",
    "json-2-csv": "^3.17.1",
    "bull": "^4.10.4",
    "redis": "^4.6.5",
    "socket.io": "^4.6.1",
    "express-validator": "^6.15.0",
    "connect-redis": "^6.1.3",
    "express-mongo-sanitize": "^2.2.0",
    "xss": "^1.0.14",
    "node-cron": "^3.0.2",
    "pdfkit": "^0.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.8.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0"
  },
  "optionalDependencies": {
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "xlsx": "^0.18.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!public/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "config": {
    "app": {
      "name": "QuotePro Enhanced",
      "version": "2.0.0",
      "description": "Professional Quotation Management System",
      "company": "International Pipes Technology Co LLC"
    },
    "database": {
      "host": "localhost",
      "port": 3306,
      "database": "quotepro_enhanced",
      "charset": "utf8mb4"
    },
    "export": {
      "maxFileSize": "50MB",
      "supportedFormats": ["pdf", "excel", "png", "jpg", "webp"],
      "defaultTemplate": "professional",
      "batchLimit": 50
    },
    "email": {
      "maxAttachmentSize": "25MB",
      "allowedDomains": ["gmail.com", "outlook.com", "company.com"]
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/iammuhammedboss"
  },
  "contributors": [
    {
      "name": "Muhammed Boss",
      "email": "muhammedboss@quotepro.com",
      "role": "Lead Developer"
    }
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}